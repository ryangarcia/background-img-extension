{
  "manifest_version": 2,

  "name": "Background Image Extension",
  "description": "Creates a new tab with a background image",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "newpage.html"
  },

  "browser_action": {
  },

  "permissions": [
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }

}